{
  "name": "react-use-focus-trap",
  "version": "1.1.6",
  "description": "A react hook to trap the focus within a reference",
  "keywords": [
    "react",
    "modal",
    "focus",
    "trap",
    "focus-trap",
    "a11y",
    "accessibility"
  ],
  "source": "src/useFocusTrap.js",
  "type": "module",
  "targets": {
    "commonjs": {
      "includeNodeModules": {
        "react": false
      },
      "isLibrary": true,
      "outputFormat": "commonjs"
    },
    "esm": {
      "includeNodeModules": {
        "react": false
      },
      "outputFormat": "esmodule",
      "isLibrary": true
    }
  },
  "exports": {
    ".": {
      "require": "./dist/commonjs/useFocusTrap.js",
      "import": "./dist/esm/useFocusTrap.js"
    }
  },
  "scripts": {
    "test": "mocha -r jsdom-global/register",
    "build": "parcel build",
    "run:demo": "npm install && npm run build && npm link && cd demo && npm install && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activenode/use-focus-trap.git"
  },
  "author": "David Lorenz <info@activenode.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/activenode/use-focus-trap/issues"
  },
  "homepage": "https://github.com/activenode/use-focus-trap#readme",
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "esbuild": "^0.14.5",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.1.3",
    "parcel": "^2.7.0",
    "react": "^17.0.2"
  }
}
